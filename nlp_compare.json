        "ComplianceOfficer#Claims@odata.type": "#Collection(String)",
        "Created": "2022-02-17T07:56:58Z",
        "Editor": {
          "@odata.type": "#Microsoft.Azure.Connectors.SharePoint.SPListExpandedUser",
          "Claims": "i:0#.f|membership|cid_functional_account@philips.com",
          "DisplayName": "CID Functional Account (Functional Account)",
          "Email": "CID_Functional_Account@philips.com",
